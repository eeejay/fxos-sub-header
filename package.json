{
  "name": "gaia-sub-header",
  "version": "0.4.0",
  "homepage": "https://github.com/gaia-components/gaia-sub-header",
  "license": "MIT",
  "main": "gaia-sub-header.js",
  "devDependencies": {
    "babel": "^5.2.12",
    "bower": "^1.4.1",
    "browserify": "^10.1.0",
    "jshint": "^2.7.0",
    "karma": "^0.12.31",
    "karma-firefox-launcher": "^0.1.4",
    "karma-mocha": "^0.1.10",
    "karma-sinon-chai": "^0.3.0",
    "mozilla-download": "^1.0.5"
  },
  "scripts": {
    "install": "bower install",
    "lint": "./node_modules/jshint/bin/jshint gaia-sub-header.js",
    "bundle": "mkdir -p dist && browserify gaia-sub-header.js --outfile ./dist/gaia-sub-header.js --standalone GaiaSubHeader",
    "es5": "mkdir -p dist && babel ./dist/gaia-sub-header.js --out-file ./dist/gaia-sub-header-es5.js",
    "build": "npm run bundle && npm run es5"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/gaia-components/gaia-sub-header.git"
  },
  "bugs": {
    "url": "https://github.com/gaia-components/gaia-sub-header/issues"
  },
  "browser": {
    "gaia-component": "./bower_components/gaia-component/gaia-component.js"
  }
}
